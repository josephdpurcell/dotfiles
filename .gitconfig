	name = Joseph D. Purcell
	email = josephdpurcell@gmail.com
[core]
	quotepath = false
[alias]                                                                                                                            
    st = status                                                                                                                    
    ci = commit                                                                                                                    
    br = branch                                                                                                                    
    co = checkout                                                                                                                  
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short                                                        
    type = cat-file -t                                                                                                             
    dump = cat-file -p   
    unstage = reset HEAD
    wc = whatchanged
